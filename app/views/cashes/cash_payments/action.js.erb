<% if @payments.present? %>
  $('#modal form').replaceWith('<%= j render('form') %>');
<% else %>
  $('#cash_payments').html('<%= j render(template: 'cashes/cash_payments/index', formats: [:html]) %>');
  $('#cash_balance').html('<%= number_to_currency @cash.balance %>');
  $('#modal').modal('hide');
<% end %>
